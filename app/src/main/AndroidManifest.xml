<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.nodalalapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/roomicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity android:name=".hotelinfo"></activity>
        <activity android:name=".hoteldetail" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1876688569846867~4088763528" />

        <activity android:name=".foodorder" />
        <activity android:name=".Roomser" />
        <activity android:name=".Food" />
        <activity android:name=".hotelmain" />
        <activity android:name=".Aboutus" />
        <activity android:name=".Profile" />
        <activity android:name=".account" />
        <activity android:name=".logsinupmid" />
        <activity
            android:name=".loginactivity"
            android:noHistory="true" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Main4Activity" />
        <activity android:name=".Main3Activity" />
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".registeractivity"
            android:noHistory="true"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splachscreen"
            android:noHistory="true"
            android:theme="@style/splashscreen" />
    </application>

</manifest>